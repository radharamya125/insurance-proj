pipeline {
    agent any
 
    stages {
        stage('Cloning') {
            steps {
                echo 'Cloining the code from the github repository.'
                git 'https://github.com/radharamya125/insurance-proj.git'

            }
        }

    stage('Building') {
            steps {
                echo 'package the given code by using maven'
                sh 'mvn clean package'
            }
            
        }







}
}
